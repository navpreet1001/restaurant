footer
	position relative
	width 100%
	padding: 0
	bottom 0
	margin 0 auto

	.footer
		float left
		width 100%
		height 200px
		padding: 24px 0 0 0
		margin 0
		background $background-texture
		border-top $border

		.footer-holder
			position relative
			width 1024px
			margin 0 auto

			h3
				float: left
				padding: 0 10px 6px 0
				margin: 0 0 10px 0
				font-weight 500
				font-size 16px
				line-height 16px
				color #000
				border-bottom 1px solid #333
				text-shadow 1px 1px 1px #000

			h5
				float left
				width 100%
				margin 0
				font-weight 450
				font-size 14px
				line-height 19px
				color #333

			h6
				float left
				width 100%
				margin 0
				font-weight 400
				font-size 14px
				line-height 17px
				color #333

			.row
				float left
				// temporary Width
				width 190px
				padding: 0 0 20px 0
				margin 0 10px

